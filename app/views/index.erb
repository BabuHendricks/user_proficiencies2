<div class="container">
  <h1>User Skills and Levels</h1>


 <table>
  <th>User</th>
  <th>Skill</th>
  <th>Years</th>
  <th>Formal?</th>
  <% if @users %>
    <% @users.each do |user| %>
      <tr>

        <td><%= user.name %></td>


        <td><% user.skills.each do |skill| %>
            <p><%= skill.name %></p>
            <% end %>
        </td>

        <td><% user.proficiencies.each do |proficiency| %>
          <p><%= proficiency.years %></p>
          <% end %>
        </td>

        <td><% user.proficiencies.each do |proficiency| %>
          <p><%= proficiency.formal %></p>
          <% end %>
        </td>
       </tr>
    <% end %>
  <% end %>
 </table>

</div>
